{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@context/*": [
                "src/Context/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@assets/*": [
                "src/assets/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@styles/*": [
                "src/style/*"
            ]
        }
    }
}